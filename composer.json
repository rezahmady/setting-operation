{
    "name": "rezahmady/setting-operation",
    "description": "setting operation for backpackforlaravel package",
    "license": "MIT",
    "authors": [{
        "name": "Reza Ahmadi Sabzevar",
        "email": "ahmadireza15@gmail.com",
        "homepage": "https://sityno.ir"
    }],
    "homepage": "https://github.com/rezahmady/setting-operation",
    "keywords": ["Laravel", "SettingOperation", "backpackforlaravel"],
    "require": {
        "backpack/crud": "^4.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Rezahmady\\SettingOperation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rezahmady\\SettingOperation\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Rezahmady\\SettingOperation\\SettingOperationServiceProvider"
            ],
            "aliases": {
                "SettingOperation": "Rezahmady\\SettingOperation\\Facades\\SettingOperation"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}